	"argparse": {
		"prefix": "parse_args()", // 在snippet列表中显示的名称
		"body": [
			"def parse_args():",
			"    import argparse",
			"    parser = argparse.ArgumentParser()",
			"    parser.add_argument('--device', type=str, default='cuda', help='running device: cuda or cpu')",
			"    parser.add_argument('--vis', type=bool, action='store_true', dest='visualize', help='visualize')",
			"    parser.add_argument('--write', type=str2bool, default=False, help='Use SummaryWriter to record the training')",
			"    parser.add_argument('--render', type=str2bool, default=False, help='Render or Not')",
			"    parser.add_argument('--Loadmodel', type=str2bool, default=False, help='Load pretrained model or Not')",
			"    parser.add_argument('--ModelIdex', type=int, default=300000, help='which model to load')",
			"    args = parser.parse_args()",
			"    return args",
		],
		"description": "parse_args()"
	},
	"sys.path": {
		"prefix": ["sys.path add path(simple)"],
		"body": [
			"from __future__ import annotations", // 在snippet列表中显示的名称
			"import sys, os, pathlib",
			"DirPathCurrent = os.path.dirname(os.path.realpath(__file__))",
			"DirPathImport = pathlib.Path(DirPathCurrent).${1:parent}.absolute().__str__()",
			"sys.path.append(DirPathImport)",
			"$2" //通过Tab在让光标跳跃到下1个placeholder
		],
		"description": "Add to sys.path"
	},
